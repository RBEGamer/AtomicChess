<script>

function getParameterByName(name, url = window.location.href) {
    name = name.replace(/[\[\]]/g, '\\$&');
    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
            results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, ' '));
  }





  $(document).ready(function () {

    var toc = getParameterByName("toc",window.location.href);
    console.log(toc);
    if(toc){
      $("#toc1").val(toc);
      $("#toc2").val(toc);
      $("#toc3").val(toc);
    }else{
      console.log("NO PARAM");
      $("#alert_holder").html('<div class="alert alert-info" role="alert">No download token available, you may not be able to download all documents.</div>');
    }

    var err = getParameterByName("error",window.location.href);
    if(err){
      err  = String(err).split("_").join(" ");
      $("#alert_holder_server").html('<div class="alert alert-warning" role="alert">'+err+'</div>');
    }
  });

</script>

