# interfaces(5) file used by ifup(8) and ifdown(8)

auto lo
iface lo inet loopback

# Wireless interfaces
auto wlan0
iface wlan0 inet dhcp
	wireless-essid any
	pre-up wpa_supplicant -i wlan0 -c /etc/wpa_supplicant.conf -B 
	post-down killall -q wpa_supplicant

# Wired interfaces
auto eth0
iface eth0 inet dhcp
